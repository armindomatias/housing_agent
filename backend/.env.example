# Rehabify Backend - Environment Variables
# Copy this file to .env and fill in the values

# OpenAI API Key (required for AI analysis)
OPENAI_API_KEY=sk-...

# Apify Token (optional - uses mock data if not set)
# Get yours at https://apify.com
APIFY_TOKEN=apify_api_...

# Supabase (optional for MVP, required for auth later)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_PASSWORD=your-password

# LangSmith (observability - optional)
# Get your API key at https://smith.langchain.com
LANGSMITH_API_KEY=lsv2_...
LANGSMITH_PROJECT=rehabify
LANGCHAIN_TRACING_V2=true

# Optional configuration
DEBUG=false
CORS_ORIGINS=["http://localhost:3000"]
